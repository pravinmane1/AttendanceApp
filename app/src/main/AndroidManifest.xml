<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.projectocean.attendanceapp">

    <application
        android:name="com.projectocean.attendanceapp.AttendanceApp"
        android:allowBackup="true"
        android:icon="@drawable/classroom"
        android:label="@string/app_name"
        android:roundIcon="@drawable/classroom"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AttendanceDayActivity" />
        <activity android:name=".SelectPresentStudentActivity" />
        <activity android:name=".ClassDetailsActivity" />
        <activity
            android:name=".CommonActivity"
            android:label="@string/title_activity_common" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>